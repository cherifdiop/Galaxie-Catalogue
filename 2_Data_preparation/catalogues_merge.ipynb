   ],
   "source": [
    "# Merge du dataframe qui contient tout les objets sauf M40 et M45\n",
    "merge_messier_Datastro_website_without_M40_M45 = messier_Datastro_website_without_M40_M45.merge(NGC_mattiaAverga_GitHub, how='left', on='NGC_IC_designation')\n",
    "merge_messier_Datastro_website_without_M40_M45.shape"
   ]
  },
